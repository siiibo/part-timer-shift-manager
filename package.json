{
  "name": "part-timer-shift-manager",
  "version": "1.0.0",
  "description": "",
  "main": "build/index.js",
  "private": true,
  "scripts": {
    "open:prod": "cross-env clasp_config_project=.clasp.prod.json pnpm clasp open",
    "open:dev": "cross-env clasp_config_project=.clasp.dev.json pnpm clasp open",
    "build": "node build.js",
    "buildpush:prod": "pnpm build && cross-env clasp_config_project=.clasp.prod.json clasp push --force",
    "buildpush:dev": "pnpm build && cross-env clasp_config_project=.clasp.dev.json clasp push --force",
    "postinstall": "mkdir -p build && cp appsscript.json build/appsscript.json && husky",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@hi-se/web-api": "https://gitpkg.now.sh/hi-se/node-slack-sdk/packages/web-api?ab078b145617d511327a8f74fe34f1d4695ff893",
    "date-fns": "^3.2.0",
    "husky": "^9.0.6",
    "lint-staged": "^15.2.0",
    "neverthrow": "^6.2.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@google/clasp": "^2.4.2",
    "@types/google-apps-script": "^1.0.76",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.19.5",
    "esbuild-gas-plugin": "^0.8.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "prettier": "3.2.4",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "packageManager": "pnpm@9.7.1+sha512.faf344af2d6ca65c4c5c8c2224ea77a81a5e8859cbc4e06b1511ddce2f0151512431dd19e6aff31f2c6a8f5f2aced9bd2273e1fed7dd4de1868984059d2c4247"
}
